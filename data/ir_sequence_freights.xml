<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_orders" model="ir.sequence">
            <field name="name">Order Sequence</field>
            <field name="code">order.freights.sequence</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">O-</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_commission" model="ir.sequence">
            <field name="name">commission Sequence</field>
            <field name="code">commission.freights.sequence</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">Com/%(range_year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_commission_captain" model="ir.sequence">
            <field name="name">commission Sequence</field>
            <field name="code">commission.captain.sequence</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">Com/%(range_year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
